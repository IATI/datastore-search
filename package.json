{
  "name": "datastore-search",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "dev:production": "vite --mode production",
    "build:development": "vite build --mode development",
    "build:production": "vite build --mode production",
    "serve": "serve -s dist",
    "preview": "vite preview",
    "cy:open": "cypress open --config-file cypress/envs/cypress.json",
    "cy:open:ci": "cypress open --config-file cypress/envs/cypress.ci.json",
    "cy:run": "cypress run --config-file cypress/envs/cypress.json",
    "cy:run-ci": "cypress run --config-file cypress/envs/cypress.ci.json",
    "lint": "eslint --ext .js,.vue --fix src",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hennge/vue3-pagination": "^1.0.17",
    "@heroicons/vue": "^1.0.6",
    "axios": "^0.26.1",
    "moment": "^2.29.1",
    "split.js": "^1.6.5",
    "vue": "^3.2.31",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.0.14",
    "vue3-datepicker": "^0.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.4",
    "@vue/cli-plugin-router": "~5.0.3",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.5.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "2.5.1",
    "serve": "^13.0.2",
    "tailwindcss": "^3.0.23",
    "vite": "^2.8.6"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.{js,css,md,vue}": "prettier --write"
  },
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8"
  }
}
