{
  "name": "datastore-search",
  "version": "1.3.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --mode development",
    "dev:production": "vite --mode production",
    "build:development": "vite build --mode development",
    "build:production": "vite build --mode production",
    "serve": "serve -s dist",
    "preview": "vite preview",
    "cy:open": "cypress open --config-file cypress/envs/cypress.json",
    "cy:open:ci": "cypress open --config-file cypress/envs/cypress.ci.json",
    "cy:run": "cypress run --config-file cypress/envs/cypress.json",
    "cy:run-ci": "cypress run --config-file cypress/envs/cypress.ci.json",
    "lint": "eslint --ext .js,.vue --fix src",
    "format": "prettier . --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hennge/vue3-pagination": "^1.0.17",
    "@heroicons/vue": "^1.0.6",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "moment": "^2.29.3",
    "split.js": "^1.6.5",
    "vue": "^3.2.37",
    "vue-gtag": "^2.0.1",
    "vue-router": "^4.0.16",
    "vue3-datepicker": "^0.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli-plugin-router": "~5.0.6",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.1.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "serve": "^13.0.2",
    "tailwindcss": "^3.1.4",
    "vite": "^2.9.12"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix",
    "*.{js,css,md,vue}": "prettier --write"
  },
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8"
  }
}
