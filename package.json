{
    "name": "datastore-search",
    "version": "1.5.0",
    "type": "module",
    "scripts": {
        "start": "npm run dev",
        "dev": "vite --mode development",
        "dev:production": "vite --mode production",
        "build:development": "vite build --mode development",
        "build:production": "vite build --mode production",
        "serve": "serve -s dist",
        "preview": "vite preview",
        "cy:open": "cypress open --config-file cypress.config.local.js",
        "cy:open-ci": "cypress open --config-file cypress.config.ci.js",
        "cy:open-dev": "cypress open --config-file cypress.config.dev.js",
        "cy:open-prod": "cypress open --config-file cypress.config.prod.js",
        "cy:run": "cypress run --config-file cypress.config.local.js",
        "cy:run-ci": "cypress run --config-file cypress.config.ci.js",
        "cy:run-dev": "cypress run --config-file cypress.config.dev.js",
        "cy:run-prod": "cypress run --config-file cypress.config.prod.js",
        "lint": "eslint --ext .js,.vue --fix src",
        "format": "prettier . --write",
        "prepare": "husky install",
        "savepo": "node locale/po/js2po.js",
        "loadpo": "node locale/po/po2js.js > locale/fr/index.js"
    },
    "dependencies": {
        "@hennge/vue3-pagination": "^1.0.17",
        "@heroicons/vue": "^2.0.11",
        "axios": "^1.0.0",
        "crypto-js": "^4.1.1",
        "date-fns": "^2.29.3",
        "esm": "^3.2.25",
        "leaflet": "^1.8.0",
        "leaflet.markercluster": "^1.5.3",
        "moment": "^2.29.4",
        "plausible-tracker": "^0.3.8",
        "pofile": "^1.1.3",
        "split.js": "^1.6.5",
        "vue": "^3.2.40",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.5",
        "vue3-datepicker": "^0.3.4"
    },
    "devDependencies": {
        "@types/leaflet": "^1.7.11",
        "@types/leaflet.markercluster": "^1.5.1",
        "@vitejs/plugin-vue": "^2.3.4",
        "@vue/cli-plugin-router": "~5.0.8",
        "autoprefixer": "^10.4.12",
        "cypress": "^10.9.0",
        "cypress-file-upload": "^5.0.8",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.6.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "postcss": "^8.4.17",
        "prettier": "2.7.1",
        "serve": "^14.0.1",
        "tailwindcss": "^3.1.8",
        "vite": "^2.9.14"
    },
    "lint-staged": {
        "*.{js,vue}": "eslint --cache --fix",
        "*.{js,css,md,vue}": "prettier --write"
    },
    "engines": {
        "node": ">=16 <17",
        "npm": ">=8"
    }
}
